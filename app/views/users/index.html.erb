<div id="product-list">
  <h1>Daftar User</h1>

  <table cellpadding="5" cellspacing="0">
  <% for user in @users %>
    <tr valign="top" class="<%= cycle('list-line-odd', 'list-line-even') %>">

    <td width="60%">
         <span class="seller-title"><%=h user.name %></span><br />
         <span class="seller-title"><%=h user.email %></span><br />
    </td>

    <td class="seller-actions">

    <%= link_to 'Show', user %><br/>
    <%= link_to 'Edit', edit_user_path(user) %><br/>
    <%= link_to 'Hapus', user,
                            :confirm => 'Yakin nih, penjual ini mau dihapus',
                            :method => :delete %>
    </td>
  </tr>
<% end %>
</table>
</div>

<br />

<%= link_to 'tambah user', new_seller_path %>

