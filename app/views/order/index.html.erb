<%= render :partial => 'navigation' %>
<table>
  <tr>
    <th>ID</th>
    <th>Nama</th>
    <th>Email</th>
    <th>Alamat</th>
    <th>Created at</th>
    <th>Updated at</th>
    <th></th>
  </tr>
<% for order in @orders %>
  <tr>
    <td align="right"><%= order.id %></td>
    <td align="right"><%= order.nama %></td>
    <td align="right"><%= order.email %></td>
    <td align="right"><%= order.alamat %></td>
    <td><%= button_to "View", :action => 'show', :id => order %></td>
  </tr>
<% end %>
</table>

