<table class="profile">
  <tr>
    <td class="main">
      <h2>
         <%= gravatar_for @seller %>
         <%= h @seller.name %>
      </h2>

    </td>
    <td class="sidebar round">
      <strong>Name</strong>  <%= @seller.name %><br />
      <strong>Email</strong>  <%= @seller.email %><br />
      <strong>Kota</strong>  <%= @seller.alamat %><br />
      <strong>URL</strong>  <%= link_to seller_path(@seller), seller_path(@seller) %>
    </td>
  </tr>
</table>

<%= link_to 'Edit', edit_seller_path(@seller) %> |
<%= link_to 'Back', sellers_path %>

