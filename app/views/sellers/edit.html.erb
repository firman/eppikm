<h1>Editing seller</h1>

<% form_for(@seller) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :alamat %><br />
    <%= f.text_area :alamat %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @seller %> |
<%= link_to 'Back', sellers_path %>

