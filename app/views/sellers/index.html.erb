<div id="product-list">
  <h1>Daftar Pengusaha Meubel</h1>
  <table cellpadding="5" cellspacing="0">
  <% for seller in @sellers %>
    <tr valign="top" class="<%= cycle('list-line-odd', 'list-line-even') %>">

    <td width="60%">
         <span class="seller-title"><%=h seller.name %></span><br />
         <span class="seller-title"><%=h seller.email %></span><br />
    </td>

    <td class="seller-actions">

    <%= link_to 'Show', seller %><br/>
    <%= link_to 'Edit', edit_seller_path(seller) %><br/>
    <%= link_to 'Hapus', seller,
                            :confirm => 'Yakin nih, penjual ini mau dihapus',
                            :method => :delete %>
    </td>
  </tr>
<% end %>
</table>
</div>

<br />

<%= link_to 'tambah user', new_seller_path %>

